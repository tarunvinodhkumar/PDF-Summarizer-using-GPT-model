{
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd frontend && npm run dev",
    "start": "npm-run-all --parallel start:backend start:frontend",
    "install:all": "npm install && npm-run-all --parallel install:backend install:frontend",
    "install:backend": "cd backend && npm install",
    "install:frontend": "cd frontend && npm install",
    "install-server": "cd backend && npm install",
    "start-server": "cd backend && npm start",
    "install-client": "cd frontend && npm install",
    "build-client": "cd frontend && npm run build",
    "start-client": "cd frontend && npm run dev"
  },
